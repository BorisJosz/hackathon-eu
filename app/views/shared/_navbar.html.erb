<div class="navbar">
  <%= image_tag "AskEU_LOGO.png", class: "navbar-logo"%>
  <div style="display:flex;">
    <div class="navOption">
      <%= image_tag "Question_icon.png", style: "width: 20px; height: 20px" %>
      <%= link_to "Questions", problems_path, class: "nav-link" %>
    </div> 
    <div class="navOption">
      <%= image_tag "Answer_icon.png", style: "width: 20px; height: 20px" %>
      <%= link_to "Answer", "#", class: "nav-link" %>
    </div>
  </div>
  <%= form_tag problems_path, method: :get do %>
    <div class="search-bar">
      <%= text_field_tag :query, params[:query], placeholder: "Search...", autocomplete:"off", class: "search-bar-nav" %>
    </div>
  <% end %>
  <%= link_to new_problem_path(@problem) do  %>
    <div class="navbarButton">
      <p>Add a Question</p>
    </div>
  <% end %>
  <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "nav-link" %>
    

</div>


